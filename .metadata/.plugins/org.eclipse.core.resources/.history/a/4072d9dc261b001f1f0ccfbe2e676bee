import java.util.ArrayList;

public class Arraylist {
	
	public static void main(String[] args) {
		
		ArrayList<String> arr=new ArrayList<String>();
		
		arr.add("Teja");
		arr.add("Ramesh N V");
		arr.add("Yashoda R");
		arr.add("Kiran R");
		
		for(String b: arr)
		{
			System.out.println(b);
		}
		
	}

}
